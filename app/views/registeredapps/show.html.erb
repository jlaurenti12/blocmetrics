<h1><%= @registeredapp.name %></h1>

 <div class="row">
   <div class="col-md-8">
     <p class="lead"><%= @registeredapp.url %></p>
     <h4><strong><%= @registeredapp.events.count %> events exist for this app</strong></h4>
     <% @events.each do |event| %>
 			<p>
        <%= event.first %>
      </p>
 		 <% end %>
   </div>
   <% if current_user %>
     <div class="col-md-4">
       <%= link_to "Back to all Apps", registeredapps_path, class: 'btn btn-warning' %>
       <%= link_to "Edit App", edit_registeredapp_path(@registeredapp), class: 'btn btn-success' %>
       <%= link_to "Delete App", @registeredapp, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this app?' } %>
     </div>
   <% end %>
 </div>
